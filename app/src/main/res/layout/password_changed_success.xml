<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/password_changed_success"
    android:clipToPadding="false"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    android:paddingTop="18dp"
    android:paddingBottom="28dp"
    tools:context=".NewPassword"
    android:paddingVertical="248dp">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center_horizontal"
            android:orientation="vertical"
            android:paddingHorizontal="16dp"
            android:layout_marginTop="100dp">

            <TextView
                android:id="@+id/rjdfopm79qnc"
                android:textColor="#1E232C"
                android:textSize="26sp"
                android:layout_marginBottom="13dp"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"

                android:text="Password Changed!"
                />
            <TextView
                android:textColor="#8390A1"
                android:textSize="15sp"
                android:layout_marginBottom="36dp"
                android:layout_marginStart="16dp"
                android:layout_marginEnd="16dp"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:gravity="center_horizontal"
                android:text="Your password has been changed successfully."
                />
            <LinearLayout
                xmlns:android="http://schemas.android.com/apk/res/android"
                xmlns:tools="http://schemas.android.com/tools"
                xmlns:app="http://schemas.android.com/apk/res-auto"
                android:id="@+id/rh3ijd1hqqe"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_horizontal"
                android:background="@drawable/create_new_password_reset_button"
                android:paddingVertical="21dp"
                android:layout_marginHorizontal="22dp"
                android:orientation="vertical">
                <TextView
                    android:id="@+id/rfwwmpgbudjg"
                    android:textColor="#FFFFFF"
                    android:textSize="15sp"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:text="Back to Login"
                    />
            </LinearLayout>
        </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
